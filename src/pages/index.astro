<script>
  const animate = () => {
    const gsap = window.gsap;
    const splitText = window.splitText;
    const title = document.querySelector("div.hero-title");
    const subtitle = document.querySelector("div.hero-desc");

    const a = splitText.create(title, {
      type: "words, lines",
      mask: "lines",
    });

    const b = splitText.create(subtitle, {
      type: "words, lines",
      mask: "lines",
    });
    gsap.fromTo(
      a.lines,
      { y: 100, skewX: -30 },
      {
        y: 0,
        duration: 1,
        skewX: 0,
        ease: "power3.inOut",
        stagger: 0.1,
        delay: 1.1,
      }
    );
    gsap.fromTo(
      b.words,
      { y: 100 },
      {
        y: 0,
        duration: 1,
        skewX: 0,
        ease: "power3.inOut",
        stagger: 0.01,
        delay: 1.1,
      }
    );
  };
  document.addEventListener("DOMContentLoaded", animate);
